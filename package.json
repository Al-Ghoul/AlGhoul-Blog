{
  "name": "ag-blog",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "dotenv -e .env.development ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "typesafe-i18n": "typesafe-i18n",
    "migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "deploy:dev": "dotenv -e .env.development prisma migrate deploy",
    "test:integration": "dotenv -e .env.development yarn test -- --testPathPattern='\\.integration.ts' -i --setupFilesAfterEnv",
    "test:units": "dotenv -e .env.development yarn test -- --testPathPattern='\\.unit.ts'",
    "int:build": "dotenv -e .env.development yarn build",
    "int:start": "dotenv -e .env.development yarn start"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/mdx": "^13.4.10",
    "@prisma/client": "5.6.0",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "10.4.14",
    "country-flag-icons": "^1.5.7",
    "eslint": "8.43.0",
    "eslint-config-next": "^13.4.9",
    "firebase": "^10.7.2",
    "firebase-admin": "^12.0.0",
    "flowbite": "^1.6.6",
    "flowbite-react": "^0.4.7",
    "framer-motion": "^10.12.16",
    "hastscript": "^7.2.0",
    "jest-mock-extended": "^3.0.5",
    "next": "^14.1.0",
    "next-auth": "^4.22.1",
    "next-mdx-remote": "^4.4.1",
    "postcss": "8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-highlight": "^6.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-math": "^6.0.0",
    "remark-toc": "^8.0.1",
    "string-width": "4",
    "swr": "^2.2.0",
    "tailwindcss": "3.3.2",
    "typesafe-i18n": "^5.24.3",
    "typewriter-effect": "^2.20.1",
    "wrap-ansi": "3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/jest": "^29.5.11",
    "@types/mdx": "^2.0.11",
    "@types/node": "^20.11.13",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "jest": "^29.7.0",
    "tailwind-scrollbar": "^3.0.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
